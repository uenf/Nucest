<!DOCTYPE html>
<html>
<head>
  <title>Nucest</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

    <%- flash.each do |name, msg| -%>
      <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
    <%- end -%>
<%= raw breadcrumbs.render %>
<div id="principal_content">
  <%= yield :principal_content %>
</div>

<div id="side_bar">
  <%= yield :side_bar %>
</div>

</body>
</html>

